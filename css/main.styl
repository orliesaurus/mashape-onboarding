.description
    font-size 1.2em

.image-center
    margin 0 auto
    padding 0 auto
    width 70%
    border 1px solid rgba(0,0,0,0.2)


body
    background-color #B3C7AB !important
    //#6cc148 !important
#main
    margin-top 2em
    background-color #2cc148
    border-radius 25px
    padding 2em

.logobg
    background-image url('http://a5.images.crunchbase.com/image/upload/c_limit,h_600,w_600/v1398379553/yf3vwzvtbfhtoussrslg.png')
    background-size 400px
    min-height 300px
    min-width 240px
    background-repeat no-repeat
    background-position top center
#con
    padding 0em 0em 5em 0em
.2line
  border-bottom 2px solid red
  width 128px

h1 + .text-center 
    margin-top 50px

div.text-center span
    position relative
    margin-left 40px
    border 1px green solid
    padding 10px
    background-color #6cc148
    z-index 2

.passed-page
    color white
    background-color #66CCCC !important
    position relative
    text-decoration
    z-index 2 !important

.connector
    position absolute
    top 78px
    width 100%
    border 1px solid black
    z-index 1

a span
    color white
    text-decoration none
a:visited
    text-decoration line-through
a:hover,
a:hover span
    color grey
    text-decoration none !important